<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:u="sap.ui.unified"
    xmlns:l="sap.ui.layout"
    displayBlock="true"
    controllerName="com.sap.northwindcompanyy.controller.Product"
>
    <Page
        id="page"
        title="Products"
    >
       
        <VBox id="_IDGenVBox12">
            <Toolbar>
                <Button
                    id="_IDGenButton8"
                    text="Add"
                    press="onAddPress"
                />
                <Button
                    id="_IDGenButton7"
                    text="Delete"
                    press="onDeletePress"
                />
                <Button
                    id="_IDGenButton9"
                    text="Update"
                    press="onUpdatePress"
                />
                <Button
                    icon="sap-icon://excel-attachment"
                    press="onExport"
                />
                <u:FileUploader
                    id="fileUploader"
                    uploadUrl="upload/"
                    change="onFileUploadChange"
                    placeholder="Choose a CSV file"
                />
                <ToolbarSpacer />
                <SearchField
                    width="500px"
                    search="onSearch"
                />
            </Toolbar>
 
            <Table
                id="g1"
                items="{/Products}"
                class=""
                mode="MultiSelect"
            >
                <columns>
                    <Column id="_IDGenColumn1">
                        <Text
                            id="_IDGenText1"
                            text="ProductID"
                        />
                    </Column>
                    <Column id="_IDGenColumn2">
                        <Text
                            id="_IDGenText2"
                            text="ProductName"
                        />
                    </Column>
                    <Column id="_IDGenColumn3">
                        <Text
                            id="_IDGenText3"
                            text="SupplierID"
                        />
                    </Column>
                    <Column id="_IDGenColumn4">
                        <Text
                            id="_IDGenText4"
                            text="CategoryID"
                        />
                    </Column>
                    <Column id="_IDGenColumn5">
                        <Text
                            id="_IDGenText5"
                            text="QuantityPerUnit"
                        />
                    </Column>
                    <Column id="_IDGenColumn6">
                        <Text
                            id="_IDGenText6"
                            text="UnitPrice"
                        />
                    </Column>
                    <Column id="_IDGenColumn7">
                        <Text
                            id="_IDGenText7"
                            text="UnitsInStock"
                        />
                    </Column>
                    <Column id="_IDGenColumn8">
                        <Text
                            id="_IDGenText8"
                            text="UnitsOnOrder"
                        />
                    </Column>
                    <Column id="_IDGenColumn9">
                        <Text
                            id="_IDGenText9"
                            text="ReorderLevel"
                        />
                    </Column>
                    <Column id="_IDGenColumn10">
                        <Text
                            id="_IDGenText10"
                            text="Discontinued"
                        />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem id="_IDGenColumnListItem1">
                        <cells>
                            <Text
                                id="_IDGenText01"
                                text="{ProductID}"
                            />
                            <Text
                                id="_IDGenText02"
                                text="{ProductName}"
                            />
                            <Text
                                id="_IDGenText03"
                                text="{SupplierID}"
                            />
                            <Text
                                id="_IDGenText04"
                                text="{CategoryID}"
                            />
                            <Text
                                id="_IDGenText05"
                                text="{QuantityPerUnit}"
                            />
                            <Text
                                id="_IDGenText06"
                                text="{UnitPrice}"
                            />
                           <Link
                                id="_IDGenText07"
                                text="{path: 'UnitsInStock', formatter: '.formatUnitsInStock'}"
                                press="onUnitsInStockClick"
                            />
                            <Text
                                id="_IDGenText08"
                                text="{UnitsOnOrder}"
                            />
                            <Text
                                id="_IDGenText09"
                                text="{ReorderLevel}"
                            />
                            <Text
                                id="_IDGenText010"
                                text="{Discontinued}"
                            />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </VBox>
    </Page>
</mvc:View>
 
