<mvc:View
    xmlns:microchart="sap.suite.ui.microchart"
    controllerName="com.sap.northwindcompanyy.controller.Home"
    xmlns:viz="sap.viz.ui5.controls"
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:viz.data="sap.viz.ui5.data"
    xmlns="sap.m"
>
    <App id="_IDGenApp1">
        <Page
            id="_IDGenPage1"
            title="Micro Tiles"
        >
            <IconTabBar
                id="iconTabBar"
                class="sapUiResponsiveContentPadding"
                select="onIconTabBarSelect"
            >
                <items>
                    <IconTabFilter
                        id="homeTab"
                        text="Home"
                        key="homes"
                    />
                </items>
            </IconTabBar>
            <HBox
                id="_IDGenVBox1"
                wrap="Wrap"
            >
                <GenericTile
                    id="_IDGenGenericTile1"
                    class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Territories"
                    frameType="TwoByOne"
                    subheader="RegionID"
                    press="onTerritoriesTilePress"
                >
                    <TileContent id="_IDGenTileContent11">
                        <content>
                            <microchart:ColumnMicroChart
                                id="oVizFrame"
                                columns="{path: '/Territories'}"
                            >
                                <microchart:columns>
                                    <microchart:ColumnMicroChartData
                                        id="_IDGenColumnMicroChartData1"
                                        value="{TerritoryID}"
                                        color="{Good}"
                                    />
                                    <microchart:ColumnMicroChartData
                                        id="_IDGenColumnMicroChartData2"
                                        value="{RegionID}"
                                        color="{Color}"
                                    />
                                </microchart:columns>
                            </microchart:ColumnMicroChart>
                        </content>
                    </TileContent>
                </GenericTile>

                <GenericTile
                    id="_IDGenGenericTile14"
                    class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Sales By Category"
                    frameType="TwoByOne"
                    subheader=""
                    press="onSalesByCategoriesTilePress"
                >
                    <TileContent id="_IDGenTileContent12">
                        <microchart:StackedBarMicroChart
                            size="M"
                            class="sapUiSmallMargin customMicroChart"
                            bars="{path: '/Sales_by_Categories'}"
                        >
                            <microchart:bars>
                                <microchart:StackedBarMicroChartBar
                                    value="{CategoryID}"
                                    valueColor="Default"
                                />
                            </microchart:bars>
                        </microchart:StackedBarMicroChart>
                    </TileContent>
                </GenericTile>
                <GenericTile
                    id="_IDGenGenericTile2"
                    class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Product Sale for 1997"
                    frameType="TwoByOne"
                    subheader=""
                    press="onproduct_salesPress"
                    
                >
                    <TileContent id="_IDGenTileContent2">
                         <microchart:InteractiveBarChart
            labelWidth="25%"
            displayedBars="3"
            selectionChanged="selectionChanged"
            bars="{/Product_Sales_for_1997}"
        >
            <microchart:bars>
                <microchart:InteractiveBarChartBar
                    label="{ProductName}"
                    value="{ProductSales}"
                />
            </microchart:bars>
        </microchart:InteractiveBarChart>
                        
                    </TileContent>
                </GenericTile>

                <GenericTile
                    id="_IDGenGenericTile3"
                    class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Sales Total By Amount"
                    frameType="TwoByOne"
                    subheader=""
                    press="onsales_amountPress"
                >
                    <TileContent id="_IDGenTileContent3">
                         <microchart:InteractiveBarChart
            labelWidth="25%"
            displayedBars="3"
            selectionChanged="selectionChanged"
            bars="{/Sales_Totals_by_Amounts}"
        >
            <microchart:bars>
                <microchart:InteractiveBarChartBar
                    label="{CompanyName}"
                    value="{SaleAmount}"
                    color="Critical"
                />
            </microchart:bars>
        </microchart:InteractiveBarChart>

                    </TileContent>
                </GenericTile>
            </HBox>
        </Page>
    </App>
</mvc:View>
