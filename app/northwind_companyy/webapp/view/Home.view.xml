<mvc:View
    xmlns:microchart="sap.suite.ui.microchart"
    controllerName="com.sap.northwindcompanyy.controller.Home"
    xmlns:viz="sap.viz.ui5.controls"
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:viz.data="sap.viz.ui5.data"
    xmlns="sap.m"
>
    <App id="_IDGenApp1">
        <Page
            id="_IDGenPage1"
            title="Micro Tiles"
        >
            <IconTabBar
                id="iconTabBar"
                class="sapUiResponsiveContentPadding"
                select="onIconTabBarSelect"
            >
                <items>
                    <IconTabFilter
                        id="homeTab"
                        text="Home"
                        key="homes"
                    />
                </items>
            </IconTabBar>
            <HBox
                id="_IDGenVBox1"
                wrap="Wrap"
            >
                <GenericTile
                    id="_IDGenGenericTile1"
                    class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Territories"
                    frameType="TwoByOne"
                    subheader="RegionID"
                    press="onTerritoriesTilePress"
                >
                    <TileContent
                        id="_IDGenTileContent11"
                       
                    >
                        <content>
                            <microchart:ColumnMicroChart
                                id="oVizFrame"
                                columns="{path: '/Territories'}"
                            >
                                <microchart:columns>
                                    <microchart:ColumnMicroChartData
                                        id="_IDGenColumnMicroChartData1"
                                        value="{TerritoryID}"
                                        color="{Good}"
                                    />
                                    <microchart:ColumnMicroChartData
                                        id="_IDGenColumnMicroChartData2"
                                        value="{RegionID}"
                                        color="{Color}"
                                    />
                                   
                                </microchart:columns>
                            </microchart:ColumnMicroChart>
                        </content>
                    </TileContent>
                </GenericTile>
                
                 <GenericTile
                    id="_IDGenGenericTile14"
                    class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Sales By Category"
                    frameType="TwoByOne"
                    subheader=""
                     press="onSalesByCategoriesTilePress"
                >
                    <TileContent
                        id="_IDGenTileContent12"
                    >
                    <microchart:StackedBarMicroChart
                                size="M"
                               
                                class="sapUiSmallMargin customMicroChart"
                                bars="{path: '/Sales_by_Categories'}"
                            >
                                <microchart:bars>
                                    <microchart:StackedBarMicroChartBar
                                        value="{CategoryID}"
                                        valueColor="Good"
                                    />
                                  
                                </microchart:bars>
                            </microchart:StackedBarMicroChart>
                    </TileContent>
                </GenericTile>
                <GenericTile
                    id="_IDGenGenericTile2"
                    class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Product Sale for 1997"
                    frameType="TwoByOne"
                    subheader=""
                    press="onproduct_salesPress"
                >
                    <TileContent
                        id="_IDGenTileContent2"
                    >
                        <content>
                            
                            <viz:VizFrame
                                id="_IDGenBarChart"
                                vizType="bar"
                                width="100%"
                                height="100%"
                            >
                                <viz:dataset>
                                    <viz.data:FlattenedDataset
                                        data="{path: '/Product_Sales_for_1997'}"
                                    >
                                        <viz.data:dimensions>
                                            <viz.data:DimensionDefinition
                                                name="Category"
                                                value="{ProductName}"
                                            />
                                        </viz.data:dimensions>
                                        <viz.data:measures>
                                            <viz.data:MeasureDefinition
                                                group="1"
                                                name="SalesValue"
                                                value="{ProductSales}"
                                            />
                                        </viz.data:measures>
                                    </viz.data:FlattenedDataset>
                                </viz:dataset>
                                <viz:feeds>
                                    <viz.feeds:FeedItem
                                        uid="categoryAxis"
                                        type="Dimension"
                                        values="Category"
                                    />
                                    <viz.feeds:FeedItem
                                        uid="valueAxis"
                                        type="Measure"
                                        values="SalesValue"
                                    />
                                </viz:feeds>
                            </viz:VizFrame>
                        </content>
                    </TileContent>
                </GenericTile>
            
                <GenericTile
                    id="_IDGenGenericTile3"
                    class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Sales Total By Amount"
                    frameType="TwoByOne"
                    subheader=""
                    press="onsales_amountPress"
                >
                    <TileContent
                        id="_IDGenTileContent3"
                    >
                        <content>
                            <viz:VizFrame
                                id="_IDGenLineChart"
                                vizType="line"
                                width="100%"
                                height="100%"
                            >
                                <viz:dataset>
                                    <viz.data:FlattenedDataset
                                        data="{path: '/Sales_Totals_by_Amounts'}"
                                    >
                                        <viz.data:dimensions>
                                            <viz.data:DimensionDefinition
                                                name="CompanyName"
                                                value="{CompanyName}"
                                            />
                                        </viz.data:dimensions>
                                        <viz.data:measures>
                                            <viz.data:MeasureDefinition
                                                group="1"
                                                name="SaleAmount"
                                                value="{SaleAmount}"
                                            />
                                        </viz.data:measures>
                                    </viz.data:FlattenedDataset>
                                </viz:dataset>
                                <viz:feeds>
                                    <viz.feeds:FeedItem
                                        uid="categoryAxis"
                                        type="Dimension"
                                        values="CompanyName"
                                    />
                                    <viz.feeds:FeedItem
                                        uid="valueAxis"
                                        type="Measure"
                                        values="SaleAmount"
                                    />
                                </viz:feeds>
                            </viz:VizFrame>
                        </content>
                    </TileContent>
                </GenericTile>
            </HBox>
        </Page>
    </App>
</mvc:View>